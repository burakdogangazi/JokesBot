import requests

url = "https://matchilling-chuck-norris-jokes-v1.p.rapidapi.com/jokes/random"

headers = {
    'accept': "application/json",
    'x-rapidapi-key': "87407f8dabmsh0e2f17ac80282cap14355ejsn05088e445c2b",
    'x-rapidapi-host': "matchilling-chuck-norris-jokes-v1.p.rapidapi.com"
    }

response = requests.request("GET", url, headers=headers)

print(response.text)




# //ChuckNorris
import json
import requests
import os
from dotenv import load_dotenv
load_dotenv();

CNURL = os.getenv("CNURL");
CNACCEPT = os.getenv("CNACCEPT")
CNAPIKEY = os.getenv("CNAPIKEY")
CNAPIHOST = os.getenv("CNHOST")


print(CNURL);
print(CNACCEPT)
print(CNAPIKEY)
print(CNAPIHOST)

CNHEADERS = {
    'accept': os.getenv("CNACCEPT"),
    'x-rapidapi-key': os.getenv("CNAPIKEY"),
    'x-rapidapi-host': os.getenv("CNHOST")
    
}
response_API = requests.request("GET", CNURL, headers=CNHEADERS)
print(response_API.status_code);



